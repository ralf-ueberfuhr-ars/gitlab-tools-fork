{
  "name": "@ars/gitlab-tools",
  "version": "1.0.0",
  "description": "GitLab Tools",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc && cp -r src/public/. dist && cp src/private/*.json dist",
    "create-env": "echo {\"api\": \"https://gitlab.ars.de/api/v4\",\"access-token\": \"{{access-token}}\"} > src/private/gitlab.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.ars.de:ars/schulungen/tools/gitlab.git"
  },
  "keywords": [
    "gitlab",
    "tools"
  ],
  "author": "Ralf Ueberfuhr (ARS)",
